}

<h4>Edit Record</h4>
<hr />
<div class="row">
	<div class="col-md-4">
		<form method="post">
			<input type="hidden" asp-for="pushups.Id" />
			<div class="form-group">
				<label asp-for="pushups.Date" class="control-label"></label>
				<input type="date" asp-for="pushups.Date" class="form-control" />
				<span asp-validation-for="pushups.Date" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="pushups.Quantity" class="control-label"></label>
				<input asp-for="pushups.Quantity" class="form-control" />
				<span asp-validation-for="pushups.Quantity" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Update" class="btn btn-primary" />
			</div>
		</form>
	</div>
</div>

